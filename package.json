{
  "name": "tpl-lib",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "10.x.x"
  },
  "scripts": {
    "prepublish": "npm run build",
    "build": "npm run build:reset && $(npm bin)/babel -d ./dist ./src",
    "build:reset": "rm -rf dist",
    "flow": "$(npm bin)/flow",
    "eslint": "$(npm bin)/eslint --max-warnings 300 ./",
    "setup": "tidil setup --tpl lib",
    "test": "$(npm bin)/babel-jest",
    "test:dev": "NODE_ENV=test node index.js",
    "test:watch": "$(npm bin)/jest --watch --runInBand",
    "test:ci": "npm run flow && npm run eslint && npm run test"
},
  "dependencies": {
    "axios": "0.18.0",
    "babel-polyfill": "^6.26.0",
    "tpl-lib-helpers": "latest",
    "url-join": "4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-flow": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-plugin-module-resolver": "3.1.1",
    "chai": "^4.2.0",
    "eslint": "5.3.0",
    "eslint-cli": "^1.1.1",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-promise": "3.8.0",
    "flow-bin": "0.75.0",
    "jest": "22.4.3",
    "regenerator-runtime": "^0.12.1"
  }
}
